{
    "name": "andreo/eventsauce-messenger",
    "description": "Integration symfony messenger for EventSauce.",
    "keywords": [
        "eventsauce",
        "components",
        "symfony",
        "messenger"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrzej Pakula",
            "email": "andrze.pakula@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "eventsauce/eventsauce": "^2.0",
        "symfony/messenger": "^6.0",
        "symfony/dependency-injection": "^6.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.3",
        "phpunit/phpunit": "^9.4",
        "symfony/amqp-messenger": "^6.0",
        "roave/security-advisories": "dev-latest"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Andreo\\EventSauce\\Messenger\\": "src/",
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev"
}
